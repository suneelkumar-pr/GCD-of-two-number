import java.lang.*;
import java.util.*;
import java.io.*;

class Suneel{
    static int GCD(int a,int b)
    {
        if(a==0)
         return b;
        return GCD(a%b,a);
    }
    public static void main(String args[])
    {
        int a;
        int b;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        b=  sc.nextInt();
        System.out.println(GCD(a,b));
    }
}
